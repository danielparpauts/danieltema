<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="danieltema.Views.Nissan">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center" Margin="10">
            <Image x:Name="kartinka"></Image>
            <Label x:Name="header" Text="Модели NISSAN:" FontSize="Medium" />
            <Picker x:Name="picker" SelectedIndexChanged="picker_SelectedIndexChanged">
                <Picker.Items>
                    <x:String>Nissan GTR R35</x:String>
                    <x:String>Nissan GTR R34</x:String>
                    <x:String>Nissan 370Z</x:String>
                    <x:String>Nissan 240SX</x:String>
                </Picker.Items>
            </Picker>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
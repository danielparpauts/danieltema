<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="danieltema.Views.Volkswagen">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center" Margin="10">
            <Image x:Name="kartinka"></Image>
            <Label x:Name="header" Text="Модели VOLKSWAGEN:" FontSize="Medium" />
            <Picker x:Name="picker" SelectedIndexChanged="picker_SelectedIndexChanged">
                <Picker.Items>
                    <x:String>VW Golf 4</x:String>
                    <x:String>VW Scirocco MK4</x:String>
                    <x:String>VW Tiguan R Line 2020</x:String>
                    <x:String>VW Passat B7</x:String>
                </Picker.Items>
            </Picker>1
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
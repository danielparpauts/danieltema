<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="danieltema.Views.Fort">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center" Margin="10">
            <Image x:Name="kartinka"></Image>
            <Label x:Name="header" Text="Модели FORD:" FontSize="Medium" />
            <Picker x:Name="picker" SelectedIndexChanged="picker_SelectedIndexChanged">
                <Picker.Items>
                    <x:String>Ford Mustang 19690</x:String>
                    <x:String>Ford Mustang 2010</x:String>
                    <x:String>Ford Focus RS MK3</x:String>
                    <x:String>Ford Raptor F150</x:String>
                </Picker.Items>
            </Picker>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>